generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Blogs {
  id        Int      @id @default(autoincrement())
  title     String
  thumbnail String
  url       String
  tags      String
}